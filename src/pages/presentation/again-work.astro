    <h2>–í—Ä–µ–º—è —Ä–∞–±–æ—Ç—ã</h2>

    <form id="test-fib">
      <label for="fib"> –í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç–∏ –§–∏–±–æ–Ω–∞—á—á–∏</label>

      <input
        id="fib"
        name="len"
        type="text"
        placeholder="‚Ññ"
        required
      />
      <button data-type="recursion" type="submit">Recursion</button>
      <button data-type="iteration" type="submit">Iteration</button>
      <button data-type="reset" type="submit">Reset</button>

      <div class="result"></div>
    </form>

    <script is:inline>
      const fibonacciRec = (n) => {
        if (n <= 1) {
          return n;
        }

        return fibonacciRec(n - 1) + fibonacciRec(n - 2);
      };

      const fibonacci = (n) => {
        let a = 1;
        let b = 1;

        for (let i = 3; i <= n; i++) {
          let c = a + b;
          a = b;
          b = c;
        }

        return b;
      };

      function showResult(parent, data) {
        const node = document.createElement('mark');
        node.textContent = data;
        parent.querySelector('.result').prepend(node);
      }

      function getTime(fn) {
        let t0;
        let t1;

        t0 = performance.now();
        fn();
        t1 = performance.now();

        return t1 - t0;
      }

      let time = 0;
      const formPalidrome = document.querySelector('#test-fib');

      formPalidrome.addEventListener('submit', (evt) => {
        evt.preventDefault();

        const type = evt.submitter.dataset.type;

        if (type === 'reset') {
          evt.target.querySelector('.result').innerHTML = '';
          formPalidrome.reset()
          return;
        }

        const number = +evt.target.len.value;
        let n;

        if (type === 'recursion') {
          time = getTime(() => {
            n = fibonacciRec(number);
          });
        } else if (type === 'iteration') {
          time = getTime(() => {
            n = fibonacci(number);
          });
        }

        showResult(evt.target, `Number: ${n}; ${type}: ${time} ms`);
      });
    </script>
